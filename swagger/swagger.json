    "basePath": "/v1",
    "paths": {
        "/publickv/get": {
            "get": {
                "tags": [
                    "publickv"
                ],
                "description": "find key-value by key",
                "operationId": "PublicKVController.Get",
                "parameters": [
                    {
                        "in": "query",
                        "name": "pubKey",
                        "description": "Public Key of a user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "appID",
                        "description": "appID",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "key",
                        "description": "the key of kv you want to get",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "schema": {
                            "$ref": "#/definitions/models.KVObject"
                        }
                    },
                    "403": {
                        "description": ": empty object"
                    }
                }
            }
        },
        "/publickv/putitem/{appID}/{pubKey}": {
            "post": {
                "tags": [
                    "publickv"
                ],
                "description": "Put key-value to cloud",
                "operationId": "PublicKVController.PutItem",
                "parameters": [
                    {
                        "in": "path",
                        "name": "pubKey",
                        "description": "Public key in hex",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "path",
                        "name": "appID",
                        "description": "App ID",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "key",
                        "description": "The Key",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "val",
                        "description": "The Value",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "sig",
                        "description": "signature of a message consisted tk_key_value",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{map[string]string} map[string]string"
                    },
                    "403": {
                        "description": "body is empty"
                    }
                }
            }
        }
    },
    "definitions": {
        "models.KVObject": {
            "title": "KVObject",
            "type": "object",
            "properties": {
