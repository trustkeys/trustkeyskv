{
    "swagger": "2.0",
    "info": {
        "title": "Simple KV API",
        "description": "beego has a very cool tools to autogenerate documents for your API and SimpleKV used it",
        "version": "1.0.0",
        "termsOfService": "https://123xe.vn/cms/dieukhoan",
